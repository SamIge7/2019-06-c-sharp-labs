
@{
    ViewBag.Title = "LoggedIn";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <fieldset>

        <legend>

            @if (Session["ValidateSession"] != null)
            {
                <text>

                    Welcome! You have successfully logged in!

                </text>
            }

            <a asp-controller="users" asp-action="logout">Logout</a>

        </legend>

    </fieldset>

